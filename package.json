{
  "name": "sioblog.com",
  "version": "0.1.9",
  "description": "Sio AG blog",
  "scripts": {
    "start": "node node_modules/http-server/bin/http-server dist/ -a dev.sioblog.com -o --push-state",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js  --single-run",
    "test-cov": "node node_modules/http-server/bin/http-server test/coverage -o",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor protractor.conf.js"
  },
  "keywords": [
    "blog",
    "blogging"
  ],
  "repository": "https://github.com/hugomn/sioblog-frontend",
  "author": "hugomn",
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.4.1",
    "grunt": "^0.4.5",
    "grunt-aws-s3": "^0.14.0",
    "grunt-bower-install-simple": "^0.9.3",
    "grunt-bump": "0.0.15",
    "grunt-changelog": "^0.2.2",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-contrib-htmlmin": "^0.3.0",
    "grunt-contrib-uglify": "^0.5.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-ng-constant": "^1.1.0",
    "grunt-recess": "^1.0.0",
    "gruntfile-gtx": "^0.3.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.5",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.5.0",
    "karma-jasmine": "^0.3.6",
    "karma-spec-reporter": "0.0.20",
    "protractor": "^2.1.0",
    "require-directory": "^2.0.0",
    "http-server": "https://github.com/eerwitt/http-server.git#push-state-option"
  }
}
